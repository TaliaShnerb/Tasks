{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Selenium Test",
        "program": "${workspaceFolder}/e2eTest.js",
        "runtimeExecutable": null,
        "runtimeArgs": [],
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal",
        "internalConsoleOptions": "openOnSessionStart",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "smartStep": true
      }
    ]
  }
  